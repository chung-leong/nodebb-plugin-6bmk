{
  "name": "nodebb-plugin-6bmk",
  "version": "0.1.3",
  "description": "NodeBB plugin for creating hyperlocal discussion forums",
  "main": "library.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/chung-leong/nodebb-plugin-6bmk"
  },
  "keywords": [
    "nodebb",
    "plugin",
    "6bmk"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "files": [
    "languages/",
    "lib/",
    "static/",
    "library.js",
    "plugin.json",
    "!static/lib/.eslintrc"
  ],
  "author": "Chung Leong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chung-leong/nodebb-plugin-6bmk/issues"
  },
  "homepage": "https://github.com/chung-leong/nodebb-plugin-6bmk#readme",
  "readmeFilename": "README.md",
  "nbbpm": {
    "compatibility": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.5.0",
    "@commitlint/config-angular": "17.4.4",
    "eslint": "8.36.0",
    "eslint-config-nodebb": "0.2.1",
    "eslint-plugin-import": "2.27.5",
    "husky": "8.0.3",
    "lint-staged": "13.2.0"
  },
  "dependencies": {
    "6bmk": "^0.0.10"
  }
}
